<?xml version="1.0" encoding="UTF-8"?>
<Population Size="1000" CooperativeCoevolution="true" xmlns="http://tempuri.org/Schema.xsd">
  <SelectionAlgorithm Type="FitnessProportionate"/>
  <TerminationConditions>
    <TerminationCondition Type="FitnessThreshold" Arg="19" />
    <TerminationCondition Type="FitnessPlateau" Arg="250" />
  </TerminationConditions>
  <Chromosomes>
    <PermutationChromosome Repeat="9" Name="Row%i" CrossoverRate="1" MutationRate="0.25">
      <Crossovers>
        <Crossover Type="AlternatingPosition Midpoint" />
      </Crossovers>
      <Mutations>
        <Mutation Type="Transposition Displacement" Weight="2" />
        <Mutation Type="Randomization" Weight="1" />
      </Mutations>
    </PermutationChromosome>
  </Chromosomes>
</Population>
